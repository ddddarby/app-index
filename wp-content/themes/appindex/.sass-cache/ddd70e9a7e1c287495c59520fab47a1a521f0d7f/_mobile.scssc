h1, h2, h3, h4, h5, h6 {
  margin: 0;
  padding: 0;
  font-family: "ff-din-web", verdana, sans-serif;
}
h1 {
  font-weight: 700;
}
h3, h4, h5, h6, p {font-weight: 400;}
h1 {font-size: 38px; line-height: 1.3em; }
h2 {font-size: 24px; line-height: 1.3em;}
h3 {font-size: 24px;}
h4 {font-size: 18px;}
h5 {font-size: 14px;}
h6 {font-size: 12px; letter-spacing: 0.2px;}

a {
  text-decoration: none;
  color: #000000;
  border-bottom: 1px solid #000000;
  padding-bottom: 2px;
  &:hover { padding-bottom: 4px;}
}
p { font-family: 'Merriweather', serif; line-height: 1.8em;}

// Main Styling

html, body {
  margin: 0;
  padding: 0;
  background-color: #ffffff;
}

header {
  background-color: #A9E1FF;
  text-align: center;
  padding: 1px 0 0;
  h1 { margin: 20px auto }
  h6 {margin: 10px auto}
}
.about {
  width: 90%;
  max-width: 960px;
  margin: 0 auto;
  h6 {padding:10px 0;}
}
.about-show {
  border: 4px solid #ffffff;
  display: inline-block;
  padding: 6px 10px;
  position: absolute;
  top: 15px;
  right: 15px;
  cursor: pointer;
  color: #ffffff;
  font-weight: 900;
}
.about-wrapper {
  background-color: #A9E1FF;
  padding: 20px 0 0;
  text-align: center;
}
.hidden {
  display: none;
  opacity: 0;
}

.logo {
  border-bottom: 0;
  &:hover { padding-bottom: 0px;}
  width: 220px;
  height: 30px;
  padding: 0;
  @include background-image-retina(/app-index/wp-content/themes/appindex/img/logo-posts, png, 220px, 30px);
  display: block;
  margin: 18px auto;
}

.search-input {
  background-color: #ffffff;
  border: none;
  border-top: 1px solid #EFEFEF;
  border-bottom: 1px solid #EFEFEF;
  width: 100%;
  padding: 10px 0px;
  margin: 0 auto;
  text-align: center;
  @include background-image-retina(/poop/app-index/wp-content/themes/appindex/img/search-icon, png, 20px, 20px);
  background-repeat: no-repeat;
  background-position: 4% 50%;
  font-family: "ff-din-web", verdana, sans-serif;
}

.search-wrap { overflow: hidden; }
.search-submit { display: none; }

.toggle-wrap {
  background-color: #ffffff;
  padding: 20px 0 30px;
  position: relative;
  h4 { margin: 10px auto;}
  .icon {
    color: transparent;
    display: inline-block;
    width: 40px;
    height: 40px;
    display: inline-block;
    vertical-align: middle;
  }
  .apple {
    @include background-image-retina(/app-index/wp-content/themes/appindex/img/toggle-apple-mobile, png, 40px, 40px);
  }
  .android {
    @include background-image-retina(/app-index/wp-content/themes/appindex/img/toggle-android-mobile, png, 40px, 40px);
  }
  .switch-toggle {
    position: relative;
    display: inline-block;
    vertical-align: middle;
  }
}


.landing-page article {
  margin: 0 auto;
  padding: 20px 0;
  color: rgba(0,0,0,0.6);
  width: 100%;
  h2 a, h4 a, h6 a { color: rgba(0,0,0,0.6); }
  h2 {text-transform: uppercase; margin: 4px auto;}
  h6 { display: inline-block; text-transform: uppercase; }
  a {
    border-bottom: 0;
    padding-bottom: 0px;
    &:hover { padding-bottom: 0px;}
  }
  @include transition(all, 0.1s, ease-in-out);
  &:hover {
    @include background-image-retina(/app-index/wp-content/themes/appindex/img/lighten-up-yeesh, png, 5px, 5px !important);
    @include transition(all, 0.5s, ease-in-out);
    cursor: pointer;
  }
}

.tags {
  a {color: rgba(0,0,0,0.5); font-size: 12px;}
}

.article-wrap {
  width: 90%;
  margin: 0 auto;
  padding: 0;
}

#infscr-loading {
  text-align: center;
  text-transform: none;
  margin: 0 auto;
  color: transparent;
}

footer {
  background-color: #EFFBFF;
  padding: 40px 0;
  h4 {line-height: 1.5em;}
  h6 { color: #747474;}
  p {font-size: 12px; }
}

.words {
  width: 90%;
  margin: 0 auto;
  max-width: 960px;
  text-align: center;
}

