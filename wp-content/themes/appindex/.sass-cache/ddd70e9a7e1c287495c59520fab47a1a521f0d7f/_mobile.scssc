.logo-footer {
  display: inline-block;
  width: 200px;
  height: 82px;
  margin: 30px auto;
  border-bottom: 0;
  padding-bottom:0;
  &:hover { padding-bottom: 0px;}
  @include background-image-retina(/app-index/wp-content/themes/appindex/img/footer-logo-alt, png, 200px, 82px);
}


/* Start: Recommended Isotope styles */

/**** Isotope Filtering ****/

.isotope-item {
  z-index: 2;
}

.isotope-hidden.isotope-item {
  pointer-events: none;
  z-index: 1;
}

/**** Isotope CSS3 transitions ****/
.isotope,
.isotope .isotope-item {
  /* change duration value to whatever you like */
  -webkit-transition-duration: 0.8s;
     -moz-transition-duration: 0.8s;
      -ms-transition-duration: 0.8s;
       -o-transition-duration: 0.8s;
          transition-duration: 0.8s;
}

.isotope {
  -webkit-transition-property: height, width;
     -moz-transition-property: height, width;
      -ms-transition-property: height, width;
       -o-transition-property: height, width;
          transition-property: height, width;
}

.isotope .isotope-item {
  -webkit-transition-property: -webkit-transform, opacity;
     -moz-transition-property:    -moz-transform, opacity;
      -ms-transition-property:     -ms-transform, opacity;
       -o-transition-property:      -o-transform, opacity;
          transition-property:         transform, opacity;
}

/**** disabling Isotope CSS3 transitions ****/

.isotope.no-transition,
.isotope.no-transition .isotope-item,
.isotope .isotope-item.no-transition {
  -webkit-transition-duration: 0s;
     -moz-transition-duration: 0s;
      -ms-transition-duration: 0s;
       -o-transition-duration: 0s;
          transition-duration: 0s;
}

// Search Box Styling
.ui-corner-all
{
    -moz-border-radius: 0px 0px 0px 0px;
    border-radius: 0px 0px 0px 0px;
}
.ui-widget-content
{
    border: none;
    color: #222222;
    background-color: #fff;
}
.ui-widget
{
    font-family: Verdana,Arial,sans-serif;
    font-size: 15px;
}
.ui-menu
{
    display: block;
    float: left;
    list-style: none outside none;
    margin: 0;
    padding: 0px;
}
.ui-autocomplete
{
    cursor: default;
    position: absolute;
}
.ui-menu .ui-menu-item
{
    clear: left;
    float: left;
    margin: 0;
    padding: 0;
    width: 100%;
    border: 0;
}
.ui-menu .ui-menu-item a
{
    display: block;
    padding: 12px 0;
    text-decoration: none;
    cursor: pointer;
    background-color: #ffffff;
    text-align: center;
    border: 0;
}
.ui-menu .ui-menu-item a:hover
{
    display: block;
    padding: 12px 0;
    text-decoration: none;
    color: #000000;
    cursor: pointer;
    background: #ffffff;
    border: 0;
}
.ui-widget-content a
{
    color: #000000;
    border: 0;
    padding: 12px 0;
}

body .ui-autocomplete {
  /* font-family to all */
  border: 0;
  padding: 0;
}

body .ui-autocomplete .ui-menu-item .ui-corner-all {
   /* all <a> */
   border: 0;
   font-size: 16px;
   padding: 6px 0;
   margin: 0
}

body .ui-autocomplete .ui-menu-item .ui-state-focus {
   background: #f2f2f2 ;
   border: 0;
   color: #000000;
   padding: 6px 0;
}


// review page
.review-page {

}

.review-header-wrap {
  width: 90%;
  margin: 0 auto;
  text-align: center;
  padding: 40px 0 0;
  h1, h2 {color: rgba(0,0,0,0.8);}
}

.review-page-content-wrap {
  background-color: #ffffff;
  margin: 0 auto;
  padding: 10px 0 100px;
}

.review-page-content {
  width: 90%;
  margin: 0 auto;
  max-width: 1024px;

  img {
    width: 100%;
    height: auto;
  }
}

.featured-image {
  width: 100px;
  height: 100px;
  margin: 40px auto;
  border-radius: 8px;
  overflow: hidden;
  img {
    width: 100%;
    height: auto
  }
}
.download-wrap {
  h5 {display: inline-block}
  h6 {display: inline-block}
  margin: 40px auto 0;
  background-color: rgba(255,255,255,0.2);
  padding: 14px 0 12px;
  width: 100%;
  .download {
    display: block;
    width: 90%;
    margin: 0 auto;
    max-width: 1024px;
    text-align: center
  }
  .download-icon {
    display: inline-block;
    width: 14px;
    height: 14px;
    vertical-align: top;
    margin-top: 2px;
  }
  .download-link {
    border-bottom: 0;
    &:hover {padding: 6px 8px;}
    padding: 6px 8px;
    border-radius: 4px;
    background: #ffffff;
    margin: 20px 4px;
    font-size: 14px;
  }
  .apple-icon {
    @include background-image-retina(/app-index/wp-content/themes/appindex/img/download-apple, png, 14px, 14px);
  }
  .android-icon {
    @include background-image-retina(/app-index/wp-content/themes/appindex/img/download-android, png, 14px, 14px);
  }
}

.five-second-wrap {
  padding: 20px 0;
  margin: 0 auto;
  background-color: #ffffff;
  width: 100%;
  text-align: center;
  h2 {line-height: 1.4em; margin: 0 auto;}
  h6 { margin: 30px auto;}
}
.five-second {
  width: 90%;
  margin: 0 auto;
  max-width: 1024px;
}

.edit-wrap {
  background-color: #fff;
  top: 0;
  position: absolute;
  text-align: center;
  padding: 4px 0;
  width: 100%;
  height: 24px;
  h6 {display: inline-block; font-size: 10px;}
}

.post-edit-link {
  border-bottom: 0;
  &:hover { padding: 4px 6px;}
  background-color: #90c9f1;
  color: #ffffff;
  padding: 4px 6px;
  border-radius: 4px;
  font-size: 12px;
  display: inline-block;
  text-transform: uppercase;
}


.search-page-wrap {
